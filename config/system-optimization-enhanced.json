{
  "version": "3.1",
  "lastUpdated": "2025-11-30",
  "systemOptimizations": {
    "startup": {
      "enabled": true,
      "aggressiveMode": false,
      "safeToDisablePatterns": [
        "*Adobe*Updater*",
        "*Spotify*",
        "*Skype*",
        "*Teams*Helper*",
        "*Steam*",
        "*Origin*",
        "*Epic*Launcher*",
        "*Discord*",
        "*Zoom*",
        "*Slack*",
        "*WhatsApp*",
        "*Telegram*"
      ],
      "neverDisablePatterns": [
        "*Windows*Security*",
        "*Defender*",
        "*Audio*",
        "*Network*",
        "*Power*",
        "*Critical*System*",
        "*Driver*",
        "*Antivirus*",
        "*Firewall*"
      ],
      "serviceOptimization": {
        "enabled": true,
        "safeToDisable": [
          "Fax",
          "TabletInputService",
          "WSearch",
          "RemoteRegistry",
          "Spooler"
        ],
        "setToManual": [
          "Windows Search",
          "Print Spooler",
          "Secondary Logon"
        ]
      }
    },
    "ui": {
      "enabled": true,
      "profileBased": true,
      "profiles": {
        "performance": {
          "disableAnimations": true,
          "disableTransparency": true,
          "useSmallTaskbarIcons": true,
          "hideSearchBox": true,
          "disableVisualEffects": true,
          "simplifyStartMenu": true
        },
        "balanced": {
          "disableAnimations": false,
          "disableTransparency": false,
          "useSmallTaskbarIcons": true,
          "hideSearchBox": false,
          "disableVisualEffects": false,
          "simplifyStartMenu": false
        }
      }
    },
    "disk": {
      "enabled": true,
      "cleanupTargets": [
        {
          "name": "User Temp Files",
          "path": "%TEMP%\\*",
          "recurse": true,
          "minSize": "10MB",
          "maxAge": "7days"
        },
        {
          "name": "Windows Temp Files",
          "path": "C:\\Windows\\Temp\\*",
          "recurse": true,
          "minSize": "5MB",
          "maxAge": "3days"
        },
        {
          "name": "Windows Update Cache",
          "path": "C:\\Windows\\SoftwareDistribution\\Download\\*",
          "recurse": true,
          "minSize": "50MB",
          "maxAge": "30days"
        },
        {
          "name": "Browser Cache",
          "path": "%LOCALAPPDATA%\\Microsoft\\Windows\\INetCache\\*",
          "recurse": true,
          "minSize": "20MB",
          "maxAge": "14days"
        },
        {
          "name": "Recycle Bin",
          "path": "C:\\$Recycle.Bin\\*",
          "recurse": true,
          "minSize": "100MB",
          "maxAge": "30days"
        }
      ],
      "defragmentation": {
        "enabled": true,
        "skipSSD": true,
        "scheduleOptimization": true
      }
    },
    "registry": {
      "enabled": false,
      "safeOptimizations": [
        {
          "name": "Clear Recent Documents",
          "path": "HKCU:\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Explorer\\RecentDocs",
          "action": "clearEntries"
        },
        {
          "name": "Clear Run History",
          "path": "HKCU:\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Explorer\\RunMRU",
          "action": "clearEntries"
        },
        {
          "name": "Clear Jump Lists",
          "path": "HKCU:\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Explorer\\JumpListData",
          "action": "clearEntries"
        }
      ]
    },
    "network": {
      "enabled": false,
      "tcpOptimizations": {
        "enabled": true,
        "settings": {
          "TcpAckFrequency": 1,
          "TCPNoDelay": 1,
          "TcpWindowSize": 65536
        }
      },
      "dnsOptimizations": {
        "enabled": true,
        "flushDNSCache": true,
        "optimizeDNSSettings": true
      }
    },
    "modern": {
      "windows11": {
        "enabled": true,
        "optimizations": [
          {
            "name": "Disable Windows 11 Context Menu",
            "registry": "HKCU:\\Software\\Classes\\CLSID\\{86ca1aa0-34aa-4e8b-a509-50c905bae2a2}\\InprocServer32",
            "value": "",
            "type": "String"
          },
          {
            "name": "Disable Widgets",
            "registry": "HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\Advanced",
            "key": "TaskbarDa",
            "value": 0,
            "type": "DWord"
          },
          {
            "name": "Disable Chat Button",
            "registry": "HKCU:\\Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\Advanced",
            "key": "TaskbarMn",
            "value": 0,
            "type": "DWord"
          }
        ]
      },
      "gaming": {
        "enabled": false,
        "optimizations": [
          {
            "name": "Disable Game Mode",
            "registry": "HKCU:\\Software\\Microsoft\\GameBar",
            "key": "AllowAutoGameMode",
            "value": 0,
            "type": "DWord"
          },
          {
            "name": "Disable Game Bar",
            "registry": "HKCU:\\Software\\Microsoft\\GameBar",
            "key": "UseNexusForGameBarEnabled",
            "value": 0,
            "type": "DWord"
          }
        ]
      }
    }
  },
  "performanceThresholds": {
    "diskSpace": {
      "critical": 5,
      "warning": 15,
      "optimal": 25
    },
    "memory": {
      "high": 85,
      "warning": 70,
      "optimal": 60
    },
    "startup": {
      "tooMany": 15,
      "optimal": 8,
      "minimal": 5
    }
  },
  "adaptiveSettings": {
    "enabled": true,
    "systemProfiles": {
      "lowEnd": {
        "ramThreshold": 8,
        "cpuCoresThreshold": 4,
        "aggressiveOptimization": true
      },
      "midRange": {
        "ramThreshold": 16,
        "cpuCoresThreshold": 8,
        "balancedOptimization": true
      },
      "highEnd": {
        "ramThreshold": 32,
        "cpuCoresThreshold": 12,
        "conservativeOptimization": true
      }
    }
  }
}