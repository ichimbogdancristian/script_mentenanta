{
  "ModuleName": "AppUpgrade",
  "ModuleEnabled": true,
  "Description": "Automated application upgrade management via Winget and Chocolatey package managers",
  "Version": "1.0.0",
  
  "EnabledSources": [
    "Winget",
    "Chocolatey"
  ],
  
  "ExcludePatterns": [
    "Visual Studio*",
    "Microsoft Visual Studio*",
    "Microsoft SQL Server*",
    "SQL Server*",
    "VMware Workstation*",
    "VMware Tools*",
    "Docker Desktop*",
    "Docker*",
    "Node.js*",
    "Python*",
    "Git*",
    "Microsoft .NET*",
    ".NET Core*",
    ".NET Framework*",
    "Windows SDK*",
    "Microsoft Office*",
    "Office 365*",
    "Adobe*",
    "AutoCAD*",
    "JetBrains*",
    "IntelliJ*",
    "Android Studio*"
  ],
  
  "safety": {
    "createRestorePoint": true,
    "backupConfigs": false,
    "testMode": false,
    "maxConcurrentUpgrades": 5,
    "stopOnFirstFailure": false
  },
  
  "ExecutionSettings": {
    "UpgradeTimeout": 300,
    "RetryAttempts": 2,
    "RetryDelay": 5,
    "ContinueOnError": true
  },
  
  "LoggingSettings": {
    "LogLevel": "INFO",
    "DetailedOutput": true,
    "CaptureVersionHistory": true
  },
  
  "ReportingSettings": {
    "IncludeBeforeAfterVersions": true,
    "TrackSizeChanges": false,
    "GenerateUpgradeHistory": true
  }
}